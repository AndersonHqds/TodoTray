{
 "name": "todotop",
 "version": "1.0.0",
 "main": "main.js",
 "author": "Anderson <contatoahenrique@gmail.com>",
 "description": "A todolist using tray",
 "homepage": "https://andersonhqds.github.io/anderson-dev",
 "license": "MIT",
 "build": {
  "appId": "com.electron.todotop",
  "productName": "TodoTop",
  "icon": "./build/icons",
  "linux": {
